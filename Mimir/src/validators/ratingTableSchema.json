{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "id": {
        "type": "integer"
      },
      "display_name": {
        "type": "string"
      },
      "rating": {
        "type": "number"
      },
      "avg_place": {
        "type": "number"
      },
      "avg_score": {
        "type": "number"
      },
      "games_played": {
        "type": "integer"
      },
      "winner_zone": {
        "type": "boolean"
      }
    },
    "required": [
      "id",
      "display_name",
      "rating",
      "avg_place",
      "games_played"
    ]
  }
}

