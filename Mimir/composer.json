{
  "autoload": {
    "psr-4": {
      "Common\\": "../Common/",
      "": "../Common/generated/"
    }
  },
  "repositories": [
    {
      "url": "https://github.com/ctizen/idiorm.git",
      "type": "git"
    },
    {
      "url": "https://github.com/ctizen/phinx.git",
      "type": "git"
    }
  ],
  "require": {
    "ctizen/idiorm": "@stable",
    "google/protobuf": "3.21.9",
    "guzzlehttp/psr7": "2.4.3",
    "monolog/monolog": "^3.1",
    "justinrainbow/json-schema": "^5.2",
    "ctizen/phinx": "@stable",
    "phpunit/php-code-coverage": "^9.2",
    "ext-xmlreader": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-bcmath": "*",
    "ext-memcached": "*",
    "michelf/php-markdown": "^1.9",
    "textalk/websocket": "^1.6",
    "php-http/discovery": "^1.15",
    "php-http/guzzle7-adapter": "^1.0",
    "twirp/twirp": "^0.9.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.9",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
